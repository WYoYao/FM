<script type="text/html" id="addinfo">
<div class="aite-bubble">
    <!-- 头部返回 Strat -->
    <div class="aite-title">
        <span class="returnImg" @click="back()" v-show="onBlock!='index'">
            <img src="../images/return_left.png" alt="返回图片"/>
        </span>

        <span>
            <em v-show="true">选择</em>
            <em v-text="titles[onBlock]">分类</em>
        </span>
    </div>
    <!-- 头部返回 End -->

    <!-- 头部搜索 Start  -->
    <div class="list-search" v-show="true">
        <div class="info-search-box">
            <input type="text" class="keyinput" oninput="publicMethod.changeKeyword(this)">
            <div class="icon-div">
                <i onclick="publicMethod.clearInfoPointKeyword(this,1)">x</i>
                <i onclick="myWorkOrderController.searchInfoPoint(this)">f</i>
            </div>
        </div>
        <!--<div class="search-result" v-show="commonData.publicModel.curObjType2!='init' && commonData.publicModel.curObjType2!='custom' && commonData.publicModel.searchResultLength">&lt;!&ndash; v-show="commonData.publicModel.searchResultLength"&ndash;&gt;-->
        <div class="search-result" v-show="true">
            <div class="key-result">
                <span>共搜索到
                    <em class="search-num" v-text="true"></em>个相关结果</span>
            </div>
        </div>
    </div>
    <!-- 头部搜索 End  -->

    <!-- 身体部分 Start -->
    <div class="aite-body" :class="{'result-aite-body-nosearch': true,'custom-no-searchAll':true}">
        <div class="list-body">
            <!--无复选、搜索，大类-->
            <div class="none-both initTab" v-show="onBlock=='index'">
                <div class="aite-list" @click="handler_class('Build')">
                    <div>
                        <span>建筑体</span>
                    </div>
                </div>
                <div class="aite-list" @click="handler_class('Floor')">
                    <div>
                        <span>楼层</span>
                    </div>
                </div>
                <div class="aite-list" @click="handler_class('Room')">
                    <div>
                        <span>房间</span>
                    </div>
                </div>
                <div class="aite-list" @click="handler_class('System')">
                    <div>
                        <span>系统</span>
                    </div>
                </div>
                <div class="aite-list" @click="handler_class('Equip')">
                    <div>
                        <span>设备</span>
                    </div>
                </div>
            </div>
            
            <!--树1-3级，专业、系统搜索、复选-->
            <div class="both-all">
                <!-- v-show="false"-->
                <div class="tree-body">
                    <div class="tree-box" v-show="true">
                        <ptree-normal bind="true" id="'result-equip-tree'" datasource="[]">
                            <item text="obj_name" issearch="false" child="content"></item>
                        </ptree-normal>
                    </div>
                    <div class="last-level-box" :style="'width: ' + (true ? 50 : 100/3) + '%;'">
                        <!-- <div class="aite-list result-center-level" v-show="commonData.publicModel.curLevelList.length!=0" v-for="(obj,index) in commonData.publicModel.curLevelList"
                                                                        @click="publicMethod.getInfoPointForObject(obj, index, $event, 0)"> -->
                        <div class="aite-list result-center-level" v-show="true">
                            <div :title="'obj.obj_name'">
                                <span v-text="'obj.obj_name'">建筑体</span>
                            </div>
                            <div></div>
                        </div>
                    </div>
                    <div class="info-level-box" :style="'width: ' + (true ? 50 : 100/3) + '%;'">
                        <!-- <div class="aite-list" v-for="(info,index) in commonData.publicModel.infoArray" @click="publicMethod.checkInfoPoint(info, index, 0)"> -->
                        <div class="aite-list" @click="void 0">
                            <div :title="'info.name'">
                                <span v-text="'info.name'">建筑体</span>
                            </div>
                            <div>
                                <div :id="'classSelectInfoPoint'" class="pcheckbox">
                                    <!--@click="publicMethod.checkInfoPoint(info, index, 0)"-->
                                    <span class="pcheckbox_input" :class="true ? 'pcheckbox-checked' : ''"></span>
                                    <span class="pswitch_label"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--及时搜索，只有复选-->
            <div class="timely-checkbox" v-show="true">
                <!-- <div class="aite-list" v-for="(model, index) in commonData.publicModel.curLevelList" @click="publicMethod.checkInfoPoint(model, index, 0, 1)"> -->
                <div class="aite-list">
                    <div>
                        <p v-if="true" :title="'model.info_point.name'">
                            <!-- <span v-if="model.info_point.name_arr" v-for="charObj in model.info_point.name_arr" v-text="charObj.char" :class="charObj.mark ? 'redMark' : ''">空间1</span> -->
                            <span v-if="true" v-text="'charObj.char'" :class="true ? 'redMark' : ''">空间1</span>
                            <span v-if="true" v-text="'model.info_point.name'"></span>
                        </p>
                        <!-- <p v-for="parent in model.parents" v-text="parent.linked_names" :title="parent.linked_names"></p> -->
                        <p v-text="'parent.linked_names'" :title="'parent.linked_names'"></p>
                    </div>
                    <div>
                        <div :id="'searchInfoPointCb'" class="pcheckbox">
                            <!--@click="publicMethod.checkInfoPoint(model, index, 0, 1)"-->
                            <span class="pcheckbox_input" :class="true ? 'pcheckbox-checked' : ''"></span>
                            <span class="pswitch_label"></span>
                        </div>
                    </div>
                </div>
            </div>
            <!--自定义-->
            <div class="customize">
                <div>
                    <p>说明文字</p>
                    <ptext-text placeholder="请输入要采集内容的说明或介绍">
                        <verify errtip="请输入自定义内容" verifytype="space"></verify>
                        <verify length="40" errtip="最多只能输入40个字符" verifytype="length"></verify>
                    </ptext-text>
                </div>
                <div>
                    <p>使用控件</p>
                    <div>
                        <pcombobox-normal bind="true" id="'controlSel'">
                            <header placeholder="'请选择'"></header>
                            <item datasource="[]" text="name"></item>
                        </pcombobox-normal>
                    </div>
                </div>
                <div v-show="true">
                    <div class="single-select-input">
                        <div class="selection-input" v-show="true">
                            <p>
                                <span>单位</span>
                                <em></em>
                            </p>
                            <div class="unit-div">
                                <ptext-text bind="true" placeholder="'请输入要采集数据的单位'">
                                    <verify errtip="请输入单位" verifytype="space"></verify>
                                </ptext-text>
                            </div>
                        </div>
                        <!-- <div class="selection-input" v-show="true"
                                                                        v-for="(model, index) in commonData.publicModel.customItem.items"> -->
                        <div class="selection-input" v-show="true">
                            <p>
                                <span>选项</span>
                                <em v-text="'index1'">1</em>
                            </p>
                            <div>
                                <ptext-text bind="true" value="model">
                                    <verify errtip="最多只能输入100个字符" length="100" verifytype="length"></verify>
                                </ptext-text>
                                <i @click="void 0">p</i>
                            </div>
                        </div>
                    </div>
                    <div class="self-add-select" v-show="true">
                        <div class="self-add-btn">
                            <i>J</i>添加选项
                        </div>
                    </div>
                </div>

            </div>
            <!--搜索无结果-->
            <div class="sop-nodata">
                <pnotice-nodata text="暂无数据" subtitle="哎呀，什么都没有找到！" icon="../images/sassNodata.png"></pnotice-nodata>
            </div>
            <!--loading-->
            <ploading-part data-loading="resultPopLoading" text="加载中..."></ploading-part>
        </div>
    </div>
    <!-- 身体部分 End -->

    <div class="aite-footer" :class="true?'footer-btn-noborder':''">
        <div class="customize-btn-div" v-show="true">
            <pbutton-white text="'自定义'" click="void 0" bind="true" disabled="true"></pbutton-white>
        </div>
        <div v-show="true" class="sureBtnDivs">
            <pbutton-blue text="'确定'" id="'sureBtn'" bind="true" disabled="true" click="void 0"></pbutton-blue>
        </div>
        <div v-show="true" class="sureBtnDivs">
            <pbutton-blue text="'确定'" id="'sureBtn'" bind="true" disabled="true" click="void 0"></pbutton-blue>
        </div>
        <div v-show="true" class="sureBtnDivs">
            <pbutton-blue text="确定" id="sureBtn" click="void 0"></pbutton-blue>
        </div>
    </div>
</div>
</script>